include ../menu/menu
include ../logo/logo
include ../contacts/contacts
include ../slideshow/slideshow
include ../main-content/main-content

mixin index()
	- menu = getData('menu').menu
	- slideshow = getData('slideshow').slideshow

	+b.page
		+e.menu
			+menu(menu)
		+e.logo-and-contacts
			+e.logo
				+logo
			+e.contacts
				+contacts
		+e.slideshow
			+slideshow(slideshow)

		+e.content-wrapper
			+e.main
				+e.main-content
					+main-content
				+e.main-left
					+e.phone
					+e.news
					+e.holding
					+e.links



